/* */

class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int o_ind=0;
        for(int i=0;i<nums.size();i++)
        {
            if (nums[i]!=0)
            {
                int x=nums[o_ind];
                nums[o_ind]=nums[i];
                nums[i]=x;
                o_ind++;
            }
                
                
        }
        
    }
};
